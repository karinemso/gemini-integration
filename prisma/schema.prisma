generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://admin:123456@measures-db:5432/measures"

}

model UploadedMeasures {
  id    Int     @id @default(autoincrement())
  uuid  String  @unique @default(uuid())
  type  Float
  value Float
}

model ConfirmedMeasures {
  id             Int     @id @default(autoincrement())
  uuid           String  @unique @default(uuid())
  firstValue     Float
  confirmedValue Float
}

model MeasuresTypes {
     id             Int     @id @default(autoincrement())
     typeDescription String
}