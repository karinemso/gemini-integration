generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://admin:123456@measures-db:5432/measures?schema=public"
}

model UploadedMeasures {
  id    Int     @id @default(autoincrement())
  uuid  String  @unique @default(uuid())
  type  String
  value String
}

model ConfirmedMeasures {
  id             Int     @id @default(autoincrement())
  uuid           String  @unique @default(uuid())
  firstValue     String
  confirmedValue String
}
